{
  "builds": [
    { "src": "*.txt", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/api/v1/tulip",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/v1/tulip/user",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/v1/tulip/feeddetail",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/v1/tulip/feeds",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/v1/tulip/callback",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/v1/tulip/yequ_callback",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/v1/tulip/getToken",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/v1/strava",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/v1/strava/callback",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/v1/strava/getToken",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/v1/strava/refreshToken",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/v1/strava/athlete",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/v1/strava/clubs/(?<id>\\d*?)/activities",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/v1/spider",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/v1/spider/p",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/v1/wechat",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/.well-known/pki-validation/fileauth.txt",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/tulip_bindok.html",
      "dest": "/index.js",
      "methods": ["GET"]
    }
  ]
}
